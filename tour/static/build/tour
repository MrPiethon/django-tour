window.addEventListener("load",function(){for(var a=document.getElementsByClassName("tour-wrap"),b=0;b<a.length;b++){var c=a[b].getElementsByClassName("step-circle"),d=a[b].getElementsByClassName("step-name"),e=c.length;if(0===e)break;for(var f=100/e,g=0;e>g;g++)if(c[g].style.right=f*(e-g-1)+"%",d[g].style.right="0",g!==e-1){var h=-(d[g].offsetWidth/2)+c[g].offsetWidth/2;d[g].style.marginRight=h+"px"}var i=a[b].getElementsByClassName("complete");if(i.length){var j=i[i.length-1],k=a[b].getElementsByClassName("completed")[0],l=parseFloat(j.style.right);k.style.width=100-l+"%"}var m=a[b].getElementsByClassName("tour-bar-wrap")[0];m.className=m.className.replace("hidden","")}});