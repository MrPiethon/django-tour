window.addEventListener("load",function(){for(var a=document.getElementsByClassName("tour-wrap"),b=0;b<a.length;b++){var c=a[b].getElementsByClassName("tour-item-circle"),d=c.length;if(0===d)break;for(var e=100/d,f=0;d>f;f++)c[f].style.right=e*(d-f-1)+"%";var g=a[b].getElementsByClassName("complete");if(g.length){var h=g[g.length-1],i=a[b].getElementsByClassName("completed")[0],j=parseFloat(h.style.right);i.style.width=100-j+"%"}var k=a[b].getElementsByClassName("tour-bar-wrap")[0];k.className=k.className.replace("hidden","")}});